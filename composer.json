{
    "name": "picodexter/parameter-encryption-defuse-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PcdxParameterEncryptionBundle add-on for defuse/php-encryption",
    "keywords": ["config", "parameter", "encryption", "defuse/php-encryption"],
    "homepage": "https://picodexter.io/",
    "license": "MIT",
    "authors": [
        {
            "name": "picodexter",
            "homepage": "https://picodexter.io/"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "defuse/php-encryption": "^2.1",
        "picodexter/parameter-encryption-bundle": "^1.0",
        "symfony/framework-bundle": "^2.7 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Picodexter\\ParameterEncryptionDefuseBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
